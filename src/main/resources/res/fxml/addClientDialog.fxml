<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="240.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nrw.janikbau.sfm.controller.AddClientDialogController">
    <HBox prefWidth="200.0">
        <Region prefWidth="200.0" HBox.hgrow="ALWAYS" />
        <Label text="?">
            <HBox.margin>
                <Insets right="12.0" top="4.0" />
            </HBox.margin>
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
    </HBox>
    <VBox VBox.vgrow="ALWAYS">
        <Label text="Label" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" />
         </VBox.margin></Label>
    </VBox>
   <HBox spacing="14.0">
      <children>
         <TextField fx:id="textFieldClientname" promptText="%textFieldNamePromptText" HBox.hgrow="ALWAYS" />
      </children>
      <VBox.margin>
         <Insets left="14.0" right="14.0" />
      </VBox.margin>
   </HBox>
    <HBox alignment="CENTER_RIGHT" prefWidth="200.0" spacing="30.0">
        <VBox.margin>
            <Insets top="14.0" />
        </VBox.margin>
        <padding>
            <Insets bottom="14.0" right="14.0" />
        </padding>
        <Button fx:id="buttonExit" mnemonicParsing="false" onAction="#onButtonExitPressed" text="%buttonExitName" />
        <Button fx:id="buttonOK" mnemonicParsing="false" onAction="#buttonOKPressed" text="%buttonOK" />
    </HBox>
</VBox>
